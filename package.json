{
  "name": "Lili_Health",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "android": "react-native run-android",
    "ios": "react-native run-ios",
    "lint": "eslint ./src --ext .js,.jsx,.ts,.tsx",
    "start": "react-native start",
    "test": "jest",
    "clean": "react-native clean",
    "pod:i": "cd ios && pod install",
    "format": "prettier --write \"src/**/*.ts\"",
    "gradlew": "cd android && ./gradlew clean && cd ..",
    "debug": "react-native bundle --platform android --dev false --entry-file index.js --bundle-output android/app/src/main/assets/index.android.bundle --assets-dest android/app/src/main/res && yarn run post-debug",
    "post-debug": "cd android && ./gradlew clean && ./gradlew assembleDebug",
    "release": "react-native bundle --platform android --dev false --entry-file index.js --bundle-output android/app/src/main/assets/index.android.bundle --assets-dest android/app/build/intermediates/res/merged/release/ && rm -rf android/app/src/main/res/drawable-* && rm -rf android/app/src/main/res/raw/* && cd android && ./gradlew clean && ./gradlew assembleRelease && cd .. && open android/app/build/outputs/apk/release",
    "bundle": "cd android && ./gradlew bundleRelease && open app/build/outputs/bundle/release",
    "deploy:ios": "./ios/ios-deploy.sh",
    "deploy:android": "./android/android-deploy.sh"
  },
  "dependencies": {
    "@invertase/react-native-apple-authentication": "^2.2.2",
    "@react-native-async-storage/async-storage": "^1.18.0",
    "@react-native-community/checkbox": "^0.5.15",
    "@react-native-community/datetimepicker": "^7.1.0",
    "@react-native-community/slider": "^4.4.2",
    "@react-native-google-signin/google-signin": "^10.0.1",
    "@react-navigation/bottom-tabs": "^6.5.7",
    "@react-navigation/native": "^6.1.6",
    "@react-navigation/native-stack": "^6.9.12",
    "@reduxjs/toolkit": "^1.9.3",
    "axios": "^1.4.0",
    "date-fns": "^2.30.0",
    "formik": "^2.4.1",
    "react": "18.2.0",
    "react-native": "0.71.3",
    "react-native-bootsplash": "^4.7.3",
    "react-native-calendars": "^1.1299.0",
    "react-native-chart-kit": "^6.12.0",
    "react-native-dotenv": "^3.4.8",
    "react-native-fbsdk-next": "^11.2.1",
    "react-native-gesture-handler": "^2.9.0",
    "react-native-htmlview": "^0.16.0",
    "react-native-image-crop-picker": "^0.40.0",
    "react-native-keyboard-aware-scroll-view": "^0.9.5",
    "react-native-progress": "^5.0.0",
    "react-native-reanimated": "^3.0.2",
    "react-native-safe-area-context": "^4.5.3",
    "react-native-screens": "^3.20.0",
    "react-native-share": "^9.0.1",
    "react-native-splash-screen": "^3.3.0",
    "react-native-svg": "^13.9.0",
    "react-native-toast-message": "^2.1.6",
    "react-native-webview": "^11.18.1",
    "react-redux": "^8.0.5",
    "redux-persist": "^6.0.0",
    "redux-thunk": "^2.4.2",
    "yup": "^1.2.0"
  },
  "devDependencies": {
    "@babel/core": "^7.20.0",
    "@babel/preset-env": "^7.20.0",
    "@babel/runtime": "^7.20.0",
    "@react-native-community/eslint-config": "^3.2.0",
    "@tsconfig/react-native": "^2.0.2",
    "@types/jest": "^29.2.1",
    "@types/react": "^18.0.24",
    "@types/react-native-dotenv": "^0.2.0",
    "@types/react-test-renderer": "^18.0.0",
    "@typescript-eslint/eslint-plugin": "^5.54.1",
    "@typescript-eslint/parser": "^5.54.1",
    "babel-jest": "^29.2.1",
    "babel-plugin-module-resolver": "^5.0.0",
    "eslint": "^8.19.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-prettier": "^8.7.0",
    "eslint-import-resolver-typescript": "^3.5.3",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-jsx-a11y": "^6.7.1",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-react": "^7.32.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-native": "^4.0.0",
    "jest": "^29.2.1",
    "metro-react-native-babel-preset": "0.73.7",
    "prettier": "^2.4.1",
    "react-test-renderer": "18.2.0",
    "typescript": "4.8.4"
  },
  "jest": {
    "preset": "react-native"
  }
}
